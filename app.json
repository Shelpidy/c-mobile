{
   "expo": {
      "name": "commodity-mobile",
      "slug": "commodity-mobile",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "automatic",
      "splash": {
         "image": "./assets/splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },

      "packagerOpts": {
         "hostType": "tunnel",
         "lanType": "ip",
         "lanIp": "192.168.0.102"
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "supportsTablet": true
      },
      "android": {
         "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#ffffff"
         }
      },
      "web": {
         "favicon": "./assets/favicon.png"
      }
   }
}
